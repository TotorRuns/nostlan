{
  "author": "quinton-ashley",
  "bin": {
    "nostlan": "app.js"
  },
  "bugs": {
    "url": "https://github.com/quinton-ashley/nostlan/issues"
  },
  "build": {
    "appId": "com.qashto.nostlan",
    "copyright": "Copyright Â© 2018-2020 Quinton Ashley",
    "electronVersion": "11.2.3",
    "electronDownload": {
      "version": "11.2.3"
    },
    "mac": {
      "category": "public.app-category.game",
      "target": [
        {
          "target": "zip",
          "arch": [
            "arm64"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    },
    "linux": {
      "category": "Utility",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    }
  },
  "contributors": [
    {
      "name": "Quinton Ashley",
      "email": "qashto@gmail.com"
    }
  ],
  "dependencies": {
    "@popperjs/core": "*",
    "7zip-bin": "*",
    "acorn": "*",
    "await-spawn": "^4.0.1",
    "bootstrap": "^4.6.0",
    "byte-base64": "^1.1.0",
    "chrome-finder": "^1.0.7",
    "circletype": "^2.3.0",
    "contro-ui": "^3.0.2",
    "crc": "^3.8.0",
    "deep-extend": "^0.6.0",
    "delay": "^5.0.0",
    "diacritics": "^1.3.0",
    "electron-pug": "^2.0.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^9.1.0",
    "fuse.js": "^6.4.6",
    "gca-js": "^1.0.2",
    "gooder": "^1.0.4",
    "iso-639": "^0.2.2",
    "jimp": "^0.16.1",
    "jquery": "*",
    "jquery-ui-dist": "^1.12.1",
    "klaw": "^3.0.0",
    "linux-distro": "^4.0.1",
    "lodash": "*",
    "markdown-it": "^12.0.4",
    "material-design-icons-iconfont": "^6.1.0",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "mousetrap": "^1.6.5",
    "node-7z": "^2.1.2",
    "node-fetch": "^2.6.1",
    "open": "*",
    "popper.js": "^1.16.1",
    "probe-image-size": "*",
    "pug": "*",
    "puppeteer-core": "^8.0.0",
    "robotjs": "^0.6.0",
    "semver": "^7.3.4",
    "sharp": "0.27.1",
    "tabletojson": "^2.0.4",
    "tether": "^1.4.7",
    "three": "*",
    "usb": "^1.6.5",
    "yauzl-promise": "^2.1.3"
  },
  "description": "Nostlan is a front-end launcher for retro and current gen video game emulators!",
  "devDependencies": {
    "node-abi": "*"
  },
  "entry point": "app.js",
  "keywords": [
    "app"
  ],
  "license": "MIT",
  "main": "app.js",
  "maintainers": [
    {
      "name": "Quinton Ashley",
      "email": "qashto@gmail.com"
    }
  ],
  "name": "nostlan",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quinton-ashley/nostlan.git"
  },
  "scripts": {
    "start": "electron . --dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "up-outdated": "npm install --save $(npm outdated | cut -d ' ' -f -1 | sed '1d' | sed 's/$/@*/')",
    "clean": "sudo chmod -R 777 node_modules/robotjs",
    "rebuild": "rm -rf ./node_modules/sharp/vendor | npm rebuild --build-from-source --runtime=electron --target=11.2.3 --disturl=https://atom.io/download/atom-shell",
    "rebuild-m": "SHARP_IGNORE_GLOBAL_LIBVIPS=true rm -rf ./node_modules/sharp/vendor | npm rebuild --build-from-source --runtime=electron --target=11.2.3 --disturl=https://atom.io/download/atom-shell",
    "rebuild-w": "npm rebuild --build-from-source --runtime=electron --target=9.3.2 --disturl=https://atom.io/download/atom-shell",
    "dist-l": "electron-builder -l",
    "delete-tags": "git tag | xargs git tag -d",
    "delete-tags-remote": "git tag -l | xargs -n 1 git push --delete origin",
    "v": "npm version patch --force",
    "version": "git add -A",
    "postversion": "git push",
    "V": "npm version minor --force"
  },
  "version": "1.20.20"
}
